<h1>微分係数と導関数</h1>
<% if logged_in? %>
<%= form_with model: @pbar do |f| %>
<%= render 'shared/error_messages', object: f.object %>
   <%= f.hidden_field :course_id, :value => "2" %>
   <%= f.hidden_field :chapter_id, :value => "6" %>
   <%= f.hidden_field :section_id, :value => "1" %>
  <%= f.submit  "終わったら進捗を共有しよう", class: "btn btn-primary" %>
<% end %>
<% end %>